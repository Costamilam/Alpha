{
    "name": "costamilam/alpha",
    "description": "A simple PHP Framework for routing, control, database access, auth and more",
    "type": "library",
    "authors": [
        {
            "name": "Guilherme Costamilam",
            "email": "costamilam073@gmail.com"
        }
    ],
    "keywords": [
        "PHP",
        "PHP-Framework"
    ],
    "license": [
        "BSD-3-Clause"
    ],
    "require": {
        "php": ">=5.6",
        "lcobucci/jwt": "^3.2"
    },
    "require-dev": {
        "php": ">=5.6",
        "lcobucci/jwt": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Costamilam\\Alpha\\": "src/"
